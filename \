#	삭제함:        src/apis/space/getSpaceCheckNickName.ts
#	수정함:        src/apis/space/patchSpace.ts
#	수정함:        src/apis/space/postSpace.ts
#	새 파일:       src/apis/space/postSpaceInvite.ts
#	수정함:        src/apis/space/postSpaceTag.ts
#	새 파일:       src/assets/loading.gif
